local date = os.date("!*t")
local dailyKey = "Bear-" .. tostring(date.day * date.month * date.year)

local Window = MakeWindow({
    Hub = {
        Title = "Bear Hud (Premium)",
        Animation = "Bản premium"
    },
    Key = {
        KeySystem = true,
        Title = "Hệ Thống Key",
        Description = "Key thay đổi mỗi ngày!",
        KeyLink = "https://link4m.com/kjzVaRAh",
        Keys = {dailyKey},
        Notifi = {
            Notifications = true,
            CorrectKey = "Key chính xác! Chào mừng fen.",
            IncorrectKey = "Sai key rồi, xem lại đi fen!"
        }
    }
})


